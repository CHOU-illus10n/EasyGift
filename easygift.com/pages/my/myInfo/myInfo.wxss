page{
  font-size: 32rpx;
}
.section{
  padding: 25rpx;
  display: flex;
}
.section:last-child{
  border: none;
}
.section-title-co{
  width: 20%;
  float: left;
  line-height: 60px;
}
.section-title{
  width: 20%;
  float: left;
  line-height: 15px;
}
.avatar-title{
  width: 20%;
  float: left;
  line-height: 80px;
}
label{
  padding: 0 20rpx;
}

.avatar-wrapper {

  padding: 0;
  width: 56px !important;
  border-radius: 8px;
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 10px;
}

.avatar {
  display: block;
  width: 56px;
  height: 56px;
}

.btn {
  width: 680rpx;
  height: 88rpx;
  background: #F73C41;
  color: #FFFFFF;
  position: fixed;
  bottom: 55rpx;
  left: 35rpx;
  border-radius: 41.5rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 88rpx;
}
.community {
  display: flex;
  margin-top: 24rpx;
  width: 100%;
  height: 92rpx;
  background: #f1f3f8;
  border-radius: 10rpx;
  text-align: left;
  padding: 0 20rpx 0 36rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #8c9aa8;
}
.right {
  width: 30rpx;
  height: 30rpx;
}